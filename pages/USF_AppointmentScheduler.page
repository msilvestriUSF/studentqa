                $(".alert-danger").text(strikeValidationError);
                $("#learningMessageText").replaceWith("<div class=\"welcomeDesc\" id=\"learningMessageText\">"+ welcomeMessage+"</div>");
            }
            else {
                var x = document.getElementsByClassName("alert-danger");
