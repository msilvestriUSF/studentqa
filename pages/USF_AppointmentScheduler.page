console.log('phonenumber: '+inputtxt);  
  var phoneno = /^\(?([0-9]{3})\)?[-. ]?([0-9]{3})[-. ]?([0-9]{4})$/;  
  if(inputtxt.match(phoneno)) {  
      return true;  
  }  
  else {  
    return false;  
  } 
  return null; 
}
